	annote = {Amphiphilic},
	file = {Loh_Wang_January 11_Insight into the role of amphiphilic pluronic block copolymer as pore-forming.pdf:C\:\\Downloads\\Inbox\\00 Library\\SMTC-2\\storage\\FRETZR2W\\Loh_Wang_January 11_Insight into the role of amphiphilic pluronic block copolymer as pore-forming.pdf:application/pdf},
