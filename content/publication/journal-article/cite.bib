	author = {Liu, Chang and Fang, Wangxi and Chou, Shuren and Shi, Lei and Fane, Anthony G. and Wang, Rong},
	month = feb,
	keywords = {Forward osmosis, FO hollow fiber membrane, Layer-by-layer polyelectrolyte deposition, Low concentration draw solution},
	pages = {147--153},
