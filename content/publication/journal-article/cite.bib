	author = {Sirkar, Kamalesh K. and Fane, Anthony G. and Wang, Rong and Wickramasinghe, Ranil},
	year = {2014},
	keywords = {Membrane bioreactor, Membrane distillation, Process intensification, Membrane contactors, Membrane processes},
