@article{J_SMTC_20b_Li,
	title = {Recovery of dissolved methane from anaerobically treated food waste leachate using solvent-based membrane contactor},
	volume = {175},
	issn = {0043-1354},
	url = {https://www.sciencedirect.com/science/article/pii/S0043135420302293},
	doi = {10.1016/j.watres.2020.115693},
	abstract = {The difficulty of dissolved methane recovery remains a major hurdle for mainstream anaerobic wastewater treatment processes. We recently proposed solvent-based membrane contactor (SMC) for high ({\textgreater}90\%) methane recovery over a wide temperature range and net-energy production. Here, we investigate the methane recovery efficacy of the SMC process by using an AnMBR effluent from treating food waste leachate. We observed almost identical methane transfer kinetics to the process employing foulant-free methane-saturated feed solutions, with {\textgreater}92\% methane recoveries, showing that organic foulants have insignificant impacts on the methane transport in the SMC. We then performed two different membrane contactor experiments: direct-contact membrane-distillation (DCMD, with transmembrane water vapor flow) and SMC (no water vapor flow). From the negligible fouling observed in the SMC experiment, opposite to the DCMD, we elucidate that the absence of water vapor flow renders the SMC process intrinsically robust to membrane fouling. With the low fouling propensity of the SMC process under highly fouling environments, our study highlights the feasibility of SMC processes to enhance the energy production in mainstream anaerobic wastewater treatment processes.},
	language = {en},
	urldate = {2020-06-25},
	journal = {Water Research},
	author = {Li, Xuesong and Dutta, Abhishek and Saha, Swakshar and Lee, Hyung-Sool and Lee, Jongho},
	month = may,
	year = {2020},
	keywords = {Recovery, Anaerobic treatment, AnMBR effluent, Dissolved methane, Solvent-based membrane contactor},
	pages = {115693},
	file = {Li_2020_Recovery of dissolved methane from anaerobically.html:C\:\\Downloads\\Inbox\\00 Library\\SMTC-2\\storage\\RBWL4NXR\\Li_2020_Recovery of dissolved methane from anaerobically.html:text/html;Li et al._2020_Recovery of dissolved methane from anaerobically t.pdf:C\:\\Downloads\\Inbox\\00 Library\\SMTC-2\\storage\\JEQ7XZVL\\Li et al._2020_Recovery of dissolved methane from anaerobically t.pdf:application/pdf},
