	keywords = {morphology, ultrafiltration membranes, delayed-die swell, gas separation, mechanical-properties, membrane contactors, membrane fabrication, microporous pvdf-hfp hollow fibers, polymer dope rheology, separation performance, shear rate, shear-stress, spinneret, spinning solution, uf membranes},
	pages = {112--122},
	annote = {1\_TIPS-1 T-PTFE 2\_TIPS-2 NT 3\_TIPS-3 NT-PTFE 4\_TIPS-4 NT-Dual ;4.1 Phase Diagram ;4.2 NIPS ;5 Modification ;5.2 PVDF-Mod ;SMTC},
