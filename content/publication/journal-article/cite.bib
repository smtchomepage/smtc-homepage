	journal = {Separation and Purification Technology},
	author = {Tian, Miao and Qiu, Changquan and Liao, Yuan and Chou, Shuren and Wang, Rong},
	year = {2013},
	keywords = {Forward osmosis, Interfacial polymerization, Thin film composite membrane, Electrospining, PVDF material},
	annote = {2\_TIPS-2 NT 8\_TIPS-8 OSN\&AC; ;4.6 E-Spin ;7 Application ;7.2 NF ;SMTC},
